# ===========================
# API Configuration
# ===========================
VITE_API_URL=http://localhost:3000/api
VITE_APP_URL=http://localhost:5173
# אם תרצה לעבור לפרודקשן בעתיד:
# VITE_API_URL=https://plusfive-backend.vercel.app/api
# VITE_APP_URL=https://plusfive-two.vercel.app

# ===========================
# Authentication
# ===========================
# תשנה למפתח JWT אמיתי כשתעלה לפרודקשן
VITE_JWT_SECRET=local_jwt_dev_key_change_me
VITE_TOKEN_EXPIRY=24h

# ===========================
# Environment
# ===========================
NODE_ENV=development
VITE_APP_ENV=development

# ===========================
# App Configuration
# ===========================
VITE_APP_NAME=FivePlus
VITE_APP_VERSION=1.0.0

# ===========================
# Feature Flags
# ===========================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# ===========================
# Stripe (מושבת זמנית)
# ===========================
STRIPE_PUBLISHABLE_KEY=""
